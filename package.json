{
  "name": "easy-fetch",
  "version": "1.0.0",
  "description": "A fetch-based request library, similar to axios",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rollup": "^2.29.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --print-width 120 --single-quote --trailing-comma all --jsx-bracket-same-line --write",
      "eslint \"src/**/*.ts\""
    ]
  },
  "author": "Preflower",
  "license": "MIT"
